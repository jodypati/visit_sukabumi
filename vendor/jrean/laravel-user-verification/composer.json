{
    "name": "jrean/laravel-user-verification",
    "description": "User Email Verification For Laravel 5.*",
    "keywords": ["laravel", "framework", "user verification", "email verification"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Ragouin",
            "email": "go@askjong.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.0.*|5.1.*|5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7.6"
    },
    "autoload": {
        "psr-4": {
            "Jrean\\UserVerification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jrean\\Tests\\UserVerification\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
